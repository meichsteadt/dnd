<p id="notice"><%= notice %></p>

<h1>Campaigns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campaigns.each do |campaign| %>
      <tr>
        <td><%= link_to campaign.name, campaign %>  </td>
        <td><%= link_to edit_campaign_path(campaign) do %><i class="material-icons">edit</i></%> <% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Campaign', new_campaign_path %>
